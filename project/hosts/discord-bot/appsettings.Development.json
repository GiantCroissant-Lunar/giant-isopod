{
  "DiscordBot": {
    "Token": "${DISCORD_BOT_TOKEN}",
    "CommandPrefix": "!",
    "StatusMessage": "Giant Isopod Dev Bot"
  },
  "Akka": {
    "ActorSystemName": "DiscordBotSystem-Dev",
    "Remote": {
      "Hostname": "0.0.0.0",
      "Port": 18080,
      "PublicHostname": "localhost"
    },
    "TargetActor": {
      "ActorSelectionPath": "akka.tcp://MainApp@localhost:18090/user/discord-gateway"
    }
  },
  "Storage": {
    "Provider": "S3",
    "S3": {
      "BucketName": "giant-isopod-audio-dev",
      "Region": "us-east-1",
      "PresignedUrlExpiryMinutes": 60
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "System": "Information",
      "Akka": "Debug",
      "Discord": "Debug"
    }
  }
}
